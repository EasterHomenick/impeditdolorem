{
  "name": "impeditdolorem",
  "version": "1.6.3",
  "description": "blindrsa-ts: A TypeScript Library for the RSA Blind Signature Protocol",
  "author": "MoniqueTorphy",
  "maintainers": [
    "Armando Faz <armfazh@cloudflare.com>"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "main": "./lib/src/index.js",
  "module": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "files": [
    "lib/src/**/*.!(tsbuildinfo)"
  ],
  "keywords": [
    "accusamus",
    "dicta",
    "signature",
    "voluptatem",
    "cryptography"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EasterHomenick/impeditdolorem.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -b",
    "test": "tsc -b test && node --experimental-vm-modules node_modules/jest/bin/jest.js --ci",
    "lint": "eslint .",
    "format": "prettier './(src|test|bench|examples)/*.ts' --write",
    "bench": "tsc -b bench && node ./lib/bench/index.js",
    "examples": "tsc -b examples && node ./lib/examples/index.js",
    "clean": "rimraf lib coverage dist"
  },
  "dependencies": {
    "cupiditateomnis": "^1.0.0",
    "perspiciatisminus": "^1.1.1",
    "quaerateius": "^1.0.0",
    "sjcl": "1.0.8"
  },
  "devDependencies": {
    "@types/benchmark": "2.1.3",
    "@types/jest": "29.5.5",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "benchmark": "2.1.4",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-security": "1.7.1",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "typescript": "5.2.2"
  },
  "bugs": {
    "url": "https://github.com/EasterHomenick/impeditdolorem.git/issues"
  }
}
